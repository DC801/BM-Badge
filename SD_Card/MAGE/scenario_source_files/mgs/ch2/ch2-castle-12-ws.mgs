include!("header.mgs")

// QUIT

ch2-ws-guess-quit {
	mutate ch2-ws-turn-value = 0;
	mutate ch2-ws-turn-status = 0;
	copy ch2-ws-count-flags;
	show serial dialog spacer;
	if (variable ch2-ws-flags-tally is 0) {
		show serial dialog {
			"<y>STEREO</>: All done for now? Well, thanks for taking a look."
		}
	} else if (variable ch2-ws-flags-tally is < $toWin) {
		mutate diff = $toWin;
		mutate diff - ch2-ws-flags-tally;
		if (variable diff is 1) {
			show serial dialog {
				"<y>STEREO</>: Oh oh oh, so close! I bet I only need <y>one more</> word before everything comes together! Try again soon, you hear?"
			}
		} else {
			show serial dialog {
				"<y>STEREO</>: Done for now? Okay, thanks for trying."
				"\tOh, you know what? I bet if you found <y>$diff$</> more words, I'd be as right as rain. Try again later, if you would?"
			}
		}
	} else if (flag ch2-ws-won is true) {
		show serial dialog {
			"<y>STEREO</>: Hey! Thanks for helping me remember who I am! Good luck out there!"
		}
	} else {
		set flag ch2-ws-won to true;
		show serial dialog {
			"<y>STEREO</>: Oh! Oh! Oh! Goodness! I think you've done it! I <bold>remember!</> I'm a <bold><y>STEREO SYSTEM</>! Hey, check this out! There's music inside me! LET'S GOOOOO!"
			" "
			# "Good for you!" : ch2-ws-cutscene
		}
	}
}

