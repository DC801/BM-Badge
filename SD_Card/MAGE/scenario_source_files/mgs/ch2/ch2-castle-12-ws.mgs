// MESSAGES

ch2-ws-turn-message {
	if (variable ch2-ws-turn-status is $miss) {
		concat serial dialog {
			"Huh? I don't think I know that word."
		}
	} else if (variable ch2-ws-turn-value is $albums) {
		concat serial dialog {
			"ALBUMS? Love your enthusiasm, but I'm a single-slot kind of guy."
		}
	} else if (variable ch2-ws-turn-value is $base) {
		concat serial dialog {
			"BASE? Hmmm. Is that how you spell that?"
		}
	} else if (variable ch2-ws-turn-value is $beats) {
		concat serial dialog {
			"BEATS? Almost, but not quite what I'm looking for."
		}
	} else if (variable ch2-ws-turn-value is $broadcasts) {
		concat serial dialog {
			"BROADCASTS? Optimistic, but I don't know about that."
		}
	} else if (variable ch2-ws-turn-value is $cassettes) {
		concat serial dialog {
			"CASSETTES? As in multiple? What decade is this?"
		}
	} else if (variable ch2-ws-turn-value is $chiptunes) {
		concat serial dialog {
			"CHIPTUNES? I think you're a smidge off, but keep trying."
		}
	} else if (variable ch2-ws-turn-value is $codecs) {
		concat serial dialog {
			"CODECS? I think you're nearly there, but not quite."
		}
	} else if (variable ch2-ws-turn-value is $frequencies) {
		concat serial dialog {
			"FREQUENCIES? Almost, but not what I'm thinking of."
		}
	} else if (variable ch2-ws-turn-value is $gramophone) {
		concat serial dialog {
			"GRAMOPHONE? Are you just making things up now?"
		}
	} else if (variable ch2-ws-turn-value is $headphone) {
		concat serial dialog {
			"HEADPHONE? Is that like a 'scissor'? Or a 'pant'?"
		}
	} else if (variable ch2-ws-turn-value is $lasers) {
