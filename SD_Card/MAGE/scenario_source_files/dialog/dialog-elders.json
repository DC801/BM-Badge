	"dialog-jackob-start": [
		{
			"alignment": "BOTTOM_LEFT",
			"portrait": "jackob",
			"name": "%Jackob%",
			"messages": [
				"Did you have a question,\n%PLAYER%?"
			]
		}
	],
	"dialog-jackob-hint-hackbook1": [
		{
			"alignment": "BOTTOM_RIGHT",
			"portrait": "mage",
			"name": "%PLAYER%",
			"messages": [
				"Well... I found this book in the inner\nsanctum, and I had a question about it."
			]
		},
		{
			"alignment": "BOTTOM_LEFT",
			"portrait": "jackob",
			"name": "%Jackob%",
			"messages": [
				"Of course. What do you want to know?"
			]
		},
		{
			"alignment": "BOTTOM_RIGHT",
			"portrait": "mage",
			"name": "%PLAYER%",
			"messages": [
				"You see, it's a book about the Big Bad,\nbut the words inside are all scrambled up."
			]
		},
		{
			"alignment": "BOTTOM_LEFT",
			"portrait": "jackob",
			"name": "%Jackob%",
			"messages": [
				"I see.",
				"Perhaps that's something the Big Bad did.\nIt would be logical for him to hide\ninformation about himself."
			]
		},
		{
			"alignment": "BOTTOM_RIGHT",
			"portrait": "mage",
			"name": "%PLAYER%",
			"messages": [
				"But what can I do to get the book back to\nnormal?"
			]
		},
		{
			"alignment": "BOTTOM_LEFT",
			"portrait": "jackob",
			"name": "%Jackob%",
			"messages": [
				"The Big Bad had powers beyond what you can\ndo with that ring alone, %PLAYER%.",
				"I suspect it's something even Ring Zero\ncannot fix."
			]
		},
		{
			"alignment": "BOTTOM_RIGHT",
			"portrait": "mage",
			"name": "%PLAYER%",
			"messages": [
				"Do you know what the book used to say,\nat least?"
			]
		},
		{
			"alignment": "BOTTOM_LEFT",
			"portrait": "jackob",
			"name": "%Jackob%",
			"messages": [
				"That book... I must have read it before,\nbut...."
			]
		}
	],
	"dialog-jackob-hint-hackbook2": [
		{
			"alignment": "BOTTOM_LEFT",
			"portrait": "jackob",
			"name": "%Jackob%",
			"messages": [
				"...."
			]
		}
	],
	"dialog-jackob-hint-hackbook3": [
		{
			"alignment": "BOTTOM_LEFT",
			"portrait": "jackob",
			"name": "%Jackob%",
			"messages": [
				"Oh, %PLAYER%, I'm sorry.",
				"I can't remember anything about what the\nbook said.",
				"I suppose it doesn't matter, either way.\nThe Big Bad is gone, perhaps for good."
			]
		},
		{
			"alignment": "BOTTOM_RIGHT",
			"portrait": "mage",
			"name": "%PLAYER%",
			"messages": [
				"But the Big Bad's name used to be in that\nbook!",
				"It made me realize that I've never heard\nhis name before. Not once!",
				"Do you know what his name is?"
			]
		},
		{
			"alignment": "BOTTOM_LEFT",
			"portrait": "jackob",
			"name": "%Jackob%",
			"messages": [
				"....",
				"I must have known it, once.",
				"But....",
				"I can't remember.",
				"I'm sorry, %PLAYER%."
			]
		}
	],
	"dialog-jackob-hint-hackbook-s": [
		{
			"alignment": "BOTTOM_RIGHT",
			"portrait": "mage",
			"name": "%PLAYER%",
