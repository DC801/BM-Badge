{
	"dialog-max": [
		{
			"alignment": "BOTTOM_LEFT",
			"portrait": "max",
			"name": "%Max Swagger%",
			"messages": [
				"Ah, yes! I know you!",
				"You're the birthday mage!"
			]
		},
		{
			"alignment": "BOTTOM_RIGHT",
			"portrait": "mage",
			"name": "%PLAYER%",
			"messages": [
				"I guess you could say that."
			]
		},
		{
			"alignment": "BOTTOM_LEFT",
			"portrait": "max",
			"name": "%Max Swagger%",
			"messages": [
				"Yes, yes!",
				"Well, you're invited to the grand opening\nof my new fashion shop!"
			]
		},
		{
			"alignment": "BOTTOM_RIGHT",
			"portrait": "mage",
			"name": "%PLAYER%",
			"messages": [
				"Oh, neat!"
			]
		},
		{
			"alignment": "BOTTOM_LEFT",
			"portrait": "max",
			"name": "%Max Swagger%",
			"messages": [
				"...Next year!"
			]
		},
		{
			"alignment": "BOTTOM_RIGHT",
			"portrait": "mage",
			"name": "%PLAYER%",
			"messages": [
				"What? Next year? Your shop isn't ready\nyet?"
			]
		},
		{
			"alignment": "BOTTOM_LEFT",
			"portrait": "max",
			"name": "%Max Swagger%",
			"messages": [
				"Oh, good heavens, no! Not yet!",
				"There's much more to do... so much more!",
				"The interior must be wallpapered and\npainted, and the new carpet cut and\nlayed....",
				"The new furniture must be brought in\nfrom Copenhagen, and that could take\nweeks alone!"
			]
		},
		{
			"alignment": "BOTTOM_RIGHT",
			"portrait": "mage",
			"name": "%PLAYER%",
			"messages": [
				"I see. Well, good luck!"
			]
		}
	],
